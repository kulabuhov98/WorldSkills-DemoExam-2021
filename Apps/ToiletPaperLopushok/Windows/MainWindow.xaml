<Window x:Class="ToiletPaperLopushok.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ToiletPaperLopushok"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800" MinHeight="500" MinWidth="800" FontFamily="Gabriola">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0" Background="#CEFFF9"/>
        <Image Source="/Resources/Logotype.png" HorizontalAlignment="Left" Margin="5"/>
        <Label x:Name="TitlePageLabel" FontSize="24" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="5"/>
       
    </Grid>
</Window>
